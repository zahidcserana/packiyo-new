{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ably/ably-php": "^1.1",
        "ably/laravel-broadcaster": "^1.0",
        "barryvdh/laravel-dompdf": "^1.0",
        "doctrine/dbal": "^3.3",
        "dyrynda/laravel-cascade-soft-deletes": "^4.1",
        "facade/ignition": "^2.3.6",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "iatstuti/laravel-cascade-soft-deletes": "4.2.*",
        "igaster/laravel_cities": "^1.3",
        "laravel-json-api/laravel": "^2.4",
        "laravel-json-api/non-eloquent": "^2.0",
        "laravel/cashier": "^13",
        "laravel/framework": "^v8.82.0",
        "laravel/pennant": "dev-master#v1.3.2-laravel8",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^v2.6.2",
        "laravel/ui": "^3.3",
        "mongodb/laravel-mongodb": "^3.8",
        "owen-it/laravel-auditing": "13.5.1",
        "picqer/php-barcode-generator": "^2.2",
        "pusher/pusher-php-server": "7.0.2",
        "ramsey/uuid": "^3.8|^4.2",
        "setasign/fpdi": "^2.6",
        "setasign/fpdi_pdf-parser": "2.1.1",
        "spatie/laravel-webhook-server": "^3.8",
        "symfony/filesystem": "^6.0",
        "tecnickcom/tcpdf": "^6.6",
        "tightenco/parental": "^1.0",
        "torann/currency": "^1.1",
        "venturecraft/revisionable": "1.*",
        "webpatser/laravel-countries": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "behat/behat": "^3.13",
        "beyondcode/laravel-dump-server": "^1.0",
        "fakerphp/faker": "^1.18",
        "filp/whoops": "^2.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.16",
        "laravel-json-api/testing": "^1.0",
        "laravel/dusk": "^6.25",
        "mockery/mockery": "^1.0",
        "php-vcr/php-vcr": "^1.6",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeds/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zahidcserana/laravel-pennant.git"
        },
        {
            "type": "composer",
            "url": "https://ndr%40packiyo.com:composer-RAi6rwDR8UP65Wg@www.setasign.com/downloads/"
        }
    ]
}
